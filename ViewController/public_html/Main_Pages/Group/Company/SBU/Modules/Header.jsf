                                                       destination="/faces/Main_Pages/Group/Company/SBU/Modules/ModuleBoard.jsf"/>-->
                                        <af:spacer width="20" height="10" id="pt_s3"/>
                                        <af:link id="pt_l2" icon="#{resource['images:back_to_home.png']}"
