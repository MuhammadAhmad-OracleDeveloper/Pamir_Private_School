                                        <af:spacer width="10" height="10" id="pt_s4"/>
                                        <!--<af:button text="Logout" id="pt_b1" icon="#{resource['images:logout.png']}"
                                                       action="#{LoginsBean.logout_action}"/>-->
                                        <af:link id="pt_l3" icon="#{resource['images:logout.png']}"
