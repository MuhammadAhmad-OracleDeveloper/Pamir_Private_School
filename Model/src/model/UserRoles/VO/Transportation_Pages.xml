TP.ID = CP.PAGE_ID      
AND     
CP.ID = RP.COMPANY_PAGES_ID                                       
AND                                     
RP.R_VIEW = 'Y'                                     
AND                                     
TP.AST = 'Y'                                     
AND                                     
RP.COMPANY_SUB_MODULE_ID = 7                                
AND                                     
RP.ROLE_ID = :get_role_id    
AND    
CP.GROUP_COMPANY_ID = :get_Cmpny_ID                          
ORDER BY TP.SR_NO ASC]]></SQLQuery>
