AND            
TM.ID = CM.MODULE_ID        
AND           
TSM.MODULE_ID = :get_Module_ID           
AND            
RSM.ROLE_ID = :get_Role_ID            
AND            
GC.ID = :get_com_mod_ID       
order by TSM.SR_NO ASC]]></SQLQuery>
