<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="Grade_Name_LOV"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  SelectList="TblGrade.ID, 
       TblGrade.GRADE_NAME, 
       TblGrade.AST"
  FromList="TBL_GRADE TblGrade"
  Where="AST='Y'"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  OrderBy="TblGrade.GRADE_NAME">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.Grade_Name_LOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <ViewAccessor
    Name="Grade_Subject_Dep_LOV1"
    ViewObjectName="model.LOV.Grade_Subject_Dep_LOV"
    RowLevelBinds="true">
    <ParameterMap>
      <PIMap Variable="getgradeid">
        <TransientExpression
          trustMode="untrusted"
          Name="ExpressionScript"><![CDATA[Id]]></TransientExpression>
      </PIMap>
    </ParameterMap>
  </ViewAccessor>
  <ViewAccessor
    Name="Section_Name_LOV1"
    ViewObjectName="model.LOV.Section_Name_LOV"
    RowLevelBinds="true">
    <ParameterMap>
      <PIMap Variable="getGrade">
        <TransientExpression
          trustMode="untrusted"
          Name="ExpressionScript"><![CDATA[Id]]></TransientExpression>
      </PIMap>
    </ParameterMap>
  </ViewAccessor>
  <EntityUsage
    Name="TblGrade"
    Entity="model.ERP.EO.TblGrade"/>
  <ViewAttribute
    Name="Id"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="TblGrade"
    AliasName="ID"/>
  <ViewAttribute
    Name="GradeName"
    PrecisionRule="true"
    EntityAttrName="GradeName"
    EntityUsage="TblGrade"
    AliasName="GRADE_NAME"/>
  <ViewAttribute
    Name="Ast"
    PrecisionRule="true"
    EntityAttrName="Ast"
    EntityUsage="TblGrade"
    AliasName="AST"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
