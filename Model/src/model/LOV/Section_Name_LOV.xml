<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="Section_Name_LOV"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  SelectList="TblSections.ID, 
       TblSections.NAME, 
       TblSections.AST, 
       TblSections.GRADE_ID"
  FromList="TBL_SECTIONS TblSections"
  Where="AST='Y'  
and TblSections.GRADE_ID = :getGrade"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.Section_Name_LOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="getGrade"
    Kind="where"
    Type="java.math.BigDecimal"/>
  <EntityUsage
    Name="TblSections"
    Entity="model.ERP.EO.TblSections"/>
  <ViewAttribute
    Name="Id"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="TblSections"
    AliasName="ID"/>
  <ViewAttribute
    Name="Name"
    PrecisionRule="true"
    EntityAttrName="Name"
    EntityUsage="TblSections"
    AliasName="NAME"/>
  <ViewAttribute
    Name="Ast"
    PrecisionRule="true"
    EntityAttrName="Ast"
    EntityUsage="TblSections"
    AliasName="AST"/>
  <ViewAttribute
    Name="GradeId"
    PrecisionRule="true"
    EntityAttrName="GradeId"
    EntityUsage="TblSections"
    AliasName="GRADE_ID"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
