<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="Terms_Name_LOV"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  SelectList="TblTerms.ID, 
       TblTerms.TERM_NAME, 
       TblTerms.AST, 
       TblTerms.ACAD_YEAR_ID"
  FromList="TBL_TERMS TblTerms"
  Where="TblTerms.ACAD_YEAR_ID = :getAcadYear 
AND AST='Y'"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.Terms_Name_LOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="getAcadYear"
    Kind="where"
    Type="java.math.BigDecimal"/>
  <EntityUsage
    Name="TblTerms"
    Entity="model.ERP.EO.TblTerms"/>
  <ViewAttribute
    Name="Id"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="TblTerms"
    AliasName="ID"/>
  <ViewAttribute
    Name="TermName"
    PrecisionRule="true"
    EntityAttrName="TermName"
    EntityUsage="TblTerms"
    AliasName="TERM_NAME"/>
  <ViewAttribute
    Name="Ast"
    PrecisionRule="true"
    EntityAttrName="Ast"
    EntityUsage="TblTerms"
    AliasName="AST"/>
  <ViewAttribute
    Name="AcadYearId"
    PrecisionRule="true"
    EntityAttrName="AcadYearId"
    EntityUsage="TblTerms"
    AliasName="ACAD_YEAR_ID"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
