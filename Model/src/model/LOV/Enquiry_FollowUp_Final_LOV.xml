    TBL_STD_ENQ.CONT_EMAIL    
FROM    
    TBL_ENQ_FOLLOW_UP,    
    TBL_STD_ENQ    
WHERE    
    TBL_STD_ENQ.ID = TBL_ENQ_FOLLOW_UP.STD_ENQ_ID AND TBL_ENQ_FOLLOW_UP.AST = 'Y'    
    AND TBL_ENQ_FOLLOW_UP.STD_ENQ_ID NOT IN (   
        SELECT    
            STD_ENQ_ID    
        FROM    
            TBL_STD_REG    
        WHERE    
            STD_ENQ_ID IS NOT NULL   
